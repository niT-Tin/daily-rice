# ‰ΩøÁî®Ctrl+q Âú®ÂëΩ‰ª§Ë°å‰πãÂâçÊ∑ªÂä†sudo
function prepend-sudo ()
{
  if [[ $BUFFER != "sudo "* ]]
  then
    BUFFER="sudo $BUFFER"; CURSOR+=5
  fi
}
zle -N prepend-sudo
bindkey '^q' prepend-sudo
