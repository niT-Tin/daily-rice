# 使用Ctrl+q 在命令行之前添加sudo
function prepend-sudo ()
{
  if [[ $BUFFER != "sudo "* ]]
  then
    BUFFER="sudo $BUFFER"; CURSOR+=5
  fi
}
zle -N prepend-sudo
bindkey '^q' prepend-sudo
bindkey '^j' autosuggest-accept

